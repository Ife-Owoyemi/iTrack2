
<% for opreq in f %>
  <% @op_c_biggest = 0 %>     
  <% complete = 0 %>
  <% g = opreq.options.all %>
  <% op_c_all = 0 %>
    <% @op_c_m_1 = 0 %>
    <% @op_c_m_2 = 0 %>
    <% @op_c_m_3 = 0 %>
    <% @op_c_m = 0 %>
    <% @op_add = 0 %>
    <% @op_c_big = 0 %>
    <% @op_c_big_1 = 0 %>
    <% @op_c_big_2 = 0 %>
    <% @op_c_big_3 = 0 %>



  <%# ------------------------------------------------------%>
  <% for option in g %> 
    
    
    <% h = option.ocourses.all %>
    <%# ------------------------------------------------------%>
    <%= render "shared/multicalc2", :h => h, :@totalc => @totalc, :totalh => totalh, :taken => taken, :taking => taking, :wtake => wtake, :@used_courses => @used_courses, :@op_c_m => @op_c_m, :@op_c_m_1 => @op_c_m_1, :@op_c_m_2 => @op_c_m_2, :@op_c_m_3 => @op_c_m_3 %> 
    <%# ------------------------------------------------------%>
    <%= render "shared/multicalc3", :opreq => opreq, :@specialtyHash => @specialtyHash, :option => option, :@used_courses => @used_courses, :@totalc => @totalc, :@op_add => @op_add, :@op_c => @op_c, :@op_c_big => @op_c_big, :@op_c_biggest => @op_c_biggest, :@op_c_m => @op_c_m, :@op_c_1 => @op_c_1, :@op_c_2 => @op_c_2, :@op_c_3 => @op_c_3, :@op_c_m_1 => @op_c_m_1, :@op_c_m_2 => @op_c_m_2, :@op_c_m_3 => @op_c_m_3, :@op_c_big_1 => @op_c_big_1, :@op_c_big_2 => @op_c_big_2, :@op_c_big_3 => @op_c_big_3 %>   
    <%# ------------------------------------------------------%>
  <% end %>
  <% @specialtyHash[:op][opreq.opreqname] = Hash.new %>

  <% @specialtyHash[:op][opreq.opreqname][:progress] = @op_c_big.to_s + "/" + @op_add.to_s %>
  <% if @op_c_big == @op_add %>
    <% @specialtyHash[:op][opreq.opreqname][:complete] = true %>
  <% else %>
    <% @specialtyHash[:op][opreq.opreqname][:complete] = false %>
  <% end %>
  <% @op_c += @op_c_big %>
  <% @totalc += @op_add  %>
  <%# ------------------------------------------------------%>
<% end %>
